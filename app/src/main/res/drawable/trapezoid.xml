        android:shape="rectangle">

        <corners
            android:topLeftRadius="0dp"
            android:topRightRadius="115dp"
            android:bottomRightRadius="115dp"
            android:bottomLeftRadius="0dp">
        </corners>
        <stroke
            android:width="11dp"
            android:color="#ffffff">
        </stroke>
        <solid
            android:color="#964040">
        </solid>
    </shape>
</item>
</layer-list>
